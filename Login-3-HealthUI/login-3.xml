android:id="@+id/login2"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:clipToOutline="true"
android:background="@drawable/login2"
 />


<!-- drawable/login2.xml -->
<vector
xmlns:android="http://schemas.android.com/apk/res/android"
