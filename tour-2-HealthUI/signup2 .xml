<!-- signup2 -->
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:id="@+id/signup2"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:clipToOutline="true"
android:background="@drawable/signup2"
 />


<!-- drawable/signup2.xml -->
<vector
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:aapt="http://schemas.android.com/aapt"
android:width="414dp"
android:height="896dp"
android:viewportWidth="414"
android:viewportHeight="896"
 >

<group>

<clip-path
android:pathData="M0 0H414V896H0V0Z"
/>

<path
android:pathData="M0 0V896H414V0"
android:fillColor="#F8FAFE"
/>

</group>

</vector>

